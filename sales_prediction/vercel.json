{
    "builds": [
        {
            "src": "sales_prediction/sales_prediction/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdasize": "15mb",
                "runtime": "python3.13"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "sales_prediction/sales_prediction/wsgi.py"
        }
    ]
}
